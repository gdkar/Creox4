<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcfg SYSTEM "http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
<kcfg>
  <kcfgfile name="kjotsrc"/>
  <include>kglobalsettings.h</include>
  <group name="kjots">
    <entry name="leftInputChannel" type="Int">
      <label>Left input channel</label>
      <default>0</default>
    </entry>
    <entry name="rightInputChannel" type="Int">
      <label>Right input channel</label>
      <default>0</default>
    </entry>
    <entry name="leaveInputDisconnected" type="Bool">
      <label>Leave input disconnected</label>
      <default>false</default>
    </entry>
    <entry name="leftOutputChannel" type="Int">
      <label>Left output channel</label>
      <default>0</default>
    </entry>
    <entry name="rightOutputChannel" type="Int">
      <label>Right output channel</label>
      <default>0</default>
    </entry>
    <entry name="leaveOutputDisconnected" type="Bool">
      <label>Leave output disconnected</label>
      <default>false</default>
    </entry>
  </group>
</kcfg>